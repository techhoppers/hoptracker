<div id="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-6">
        <h2><%= @project.title %></h2>
        <table class="table profile_info">
          <tbody>
            
            <tr>
              <td><strong><%= t(:code, :scope => [:projects]) %></strong>: <%= @project.code %></td>
            </tr>
            <tr>
              <td><strong><%= t(:status, :scope => [:projects]) %></strong>: <%= get_status(@project) || "N/A" %></td>
            </tr>
             <tr>
              <td><strong><%= t(:description, :scope => [:projects]) %></strong>:
                <br />
               <%= @project.description.html_safe || "N/A" %></td>
            </tr>
           
          </tbody>
        </table>
      </div> <%#= .col-md-12 %>

        
    </div> <%#= .row %>
  </div> <%#= .container-fluid %>
</div> <%#= content %>
